{
  "testTimeout": 60000,
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "rootDir": "../",
  "modulePaths": [
    "<rootDir>"
  ],
  "coverageDirectory": "./coverage/e2e",
  "testEnvironment": "node",
  "testRegex": ".e2e.spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
